from collections import Counter
n=int(input())
l=list(map(int,input().split()))
d=Counter(l)
if d[5]==0 and d[7]==0 and d[2]>=d[4] and d[1]==d[4]+d[6] and d[2]+d[3]==d[4]+d[6]:
    for i in range(d[4]):
        print("1 2 4")
    d[2]-=d[4]
    for i in range(d[2]):
        print("1 2 6")
    for i in range(d[3]):
        print("1 3 6")
else:
    print(-1)



        
